        <import plugin="org.eclipse.emf.ecore"/>
        <import plugin="org.eclipse.emf.edit.source"/>
        <import plugin="javax.inject"/>
        <import plugin="org.eclipse.equinox.event"/>
        <import plugin="org.apache.logging.log4j" version="2.17.1" match="greaterOrEqual"/>
